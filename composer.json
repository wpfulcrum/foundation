{
  "name": "wpfulcrum/foundation",
  "description": "Fulcrum Foundation Module - collection of foundational abstracts and utilities.",
  "type": "library",
  "keywords": [],
  "homepage": "https://github.com/wpfulcrum/foundation",
  "license": "GPL-2.0+",
  "support": {
    "issues": "https://github.com/wpfulcrum/foundation/issues",
    "source": "https://github.com/wpfulcrum/foundation"
  },
  "authors": [
    {
      "name": "Tonya Mork",
      "email": "hello@hellofromtonya.com"
    }
  ],
  "require": {
    "php": "^5.6||^7",
    "wpfulcrum/config": "^3"
  },
  "autoload": {
    "psr-4": {
      "Fulcrum\\Foundation\\": "src"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Fulcrum\\Foundation\\Tests\\": "tests/"
    },
    "files": [
      "src/utility/database.php",
      "src/utility/date-time.php",
      "src/utility/functions.php",
      "src/utility/parent-child.php",
      "src/utility/plugin-functions.php"
    ]
  },
  "require-dev": {
    "brain/monkey": "^2.0",
    "phpro/grumphp": "^0.11.6",
    "phpstan/phpstan": "^0.8.0",
    "phpunit/phpunit": "~4.8 || ~5.7.9",
    "phpmd/phpmd": "^2.6",
    "squizlabs/php_codesniffer": "^3.0",
    "jakub-onderka/php-parallel-lint": "^0.9.2",
    "sebastian/phpcpd": "^3.0",
    "povils/phpmnd": "^1.1",
    "sensiolabs/security-checker": "^4.0",
    "nikic/php-parser": "^3.0"
  },
  "config": {
    "sort-order": true
  },
  "scripts": {
    "phpcs": "\"vendor/bin/phpcs\"",
    "test": "\"vendor/bin/phpunit\""
  }
}
