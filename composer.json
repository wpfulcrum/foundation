{
  "name": "wpfulcrum/foundation",
  "description": "Fulcrum Foundation Module - collection of foundational abstracts and utilities.",
  "type": "library",
  "keywords": [],
  "homepage": "https://github.com/wpfulcrum/foundation",
  "license": "GPL-2.0+",
  "support": {
    "issues": "https://github.com/wpfulcrum/foundation/issues",
    "source": "https://github.com/wpfulcrum/foundation"
  },
  "authors": [
    {
      "name": "Tonya Mork",
      "email": "hello@hellofromtonya.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fulcrum\\Foundation\\": "src/Foundation/"
    },
    "files": [
      "src/Foundation/Utility/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Fulcrum\\Tests\\Unit\\": "tests/unit/"
    }
  },
  "require": {
    "php": "^5.6|^7",
    "wpfulcrum/config": "^3",
    "wpfulcrum/extender": "^3"
  },
  "require-dev": {
    "brain/monkey": "^2.0",
    "phpunit/phpunit": "~4.8|~5.7.9",
    "squizlabs/php_codesniffer": "^3.0",
    "sensiolabs/security-checker": "^4.0"
  },
  "config": {
    "sort-order": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "phpcs": "\"vendor/bin/phpcs\"",
    "test-unit": "\"vendor/bin/phpunit\" --testsuite unit"
  }
}
